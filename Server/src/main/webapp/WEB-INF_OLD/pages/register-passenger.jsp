<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Register Passenger Page</title>
</head>
<body>

<h1>Register Passenger form</h1>

<form action="register-passenger" method="post">
    <label>Input phone</label>
    <input type="text" name="phone">
    <label>Input pass</label>
    <input type="password" name="pass">
    <label>Input name</label>
    <input type="text" name="name">
    <label>Input country</label>
    <input type="text" name="country">
    <label>Input city</label>
    <input type="text" name="city">
    <label>Input street</label>
    <input type="text" name="street">
    <label>Input house number</label>
    <input type="text" name="houseNum">
    <input type="submit">
</form>


</body>
</html>